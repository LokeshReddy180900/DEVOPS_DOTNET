pipeline {
    agent any

    environment {
        dotnet = '/home/ubuntu/.dotnet/dotnet'
        sln = '/home/ubuntu/DEVOPS_DOTNET/MyWebApp/Program.cs'
    }

    stages {
        stage("Building"){
            steps {
                echo "Building SourceCode"
                sh '%dotnet% run %sln%'
                echo "Built Successfully"
            }
        }
    }
}